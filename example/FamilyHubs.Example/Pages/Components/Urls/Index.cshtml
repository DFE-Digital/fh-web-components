@page
@using FamilyHubs.Example.Models
@model FamilyHubs.Example.Pages.Components.Urls.IndexModel

@{
    // HtmlHelper
    var familyHubs = ViewData["FamilyHubsLayoutModel"] as FamilyHubsLayoutModel;
        <p>All family hub URLs:</p>
    <ul>
        @foreach (var keyValue in familyHubs!.FamilyHubsUiOptions.Value.Urls)
        {
            <li>
                <a href="@keyValue.Value">@keyValue.Key</a>
            </li>
        }
    </ul>
}

@Html.FamilyHubUrl(UrlKey.ConnectWeb, "/")