{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FamilyHubsUi": {
    "ServiceName": "FamilyHubs example site",
    "Phase": "Beta",
    "FeedbackUrl": "https://example.com/feedback",
    "SupportEmail": "support@example.com",
    "Analytics": {
      "CookieName": "familyhubs_example_cookies_policy",
      "CookieVersion": 1,
      "MeasurementId": "",
      "ContainerId": ""
    },
    "Header": {
      "ActionLinks": [
        {
          "Text": "My account",
          "Url": "/account/my-account"
        },
        {
          "Text": "Sign out",
          "Url": "/account/signout"
        }
      ]
    },
    "Footer": {
      "Links": [
        { "Text": "Accessibility" },
        { "Text": "Contact Us" },
        { "Text": "Cookies" },
        {
          "Text": "Feedback",
          "ConfigUrl": "FamilyHubsUi:FeedbackUrl"
        },
        { "Text": "Terms and conditions" }
      ]
    }
  }
}
